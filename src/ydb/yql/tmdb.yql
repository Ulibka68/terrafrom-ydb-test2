CREATE TABLE tmdb (
    id Uint64,
    title utf8,
    release_date Date,

    original_title Utf8,
    vote_count Uint32,
    adult Bool,
    backdrop_path Utf8,
    genre_ids Json,
    original_language Utf8,
    overview Utf8,
    popularity float,
    poster_path Utf8,
    video Bool,
    vote_average float,

 PRIMARY KEY (id)
)
